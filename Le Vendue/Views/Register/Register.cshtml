
@{
    ViewBag.Title = "Register";
}
<link href="@Url.Content("~/Content/Template/style.css")" rel="stylesheet" type="text/css" />
<link rel="stylesheet" href="@Url.Content("~/fonts/material-icon/css/material-design-iconic-font.min.css")">


<div class="main">

    <!-- Sign up form -->
    <section class="signup">
        <div class="container">
            <div class="signup-content">
                <div class="signup-form">
                    <h2 class="form-title">Sign up</h2>
                    <form method="POST" class="register-form" id="register-form">
                        <div class="form-group">

                            <label for="FirstName"><i class="zmdi zmdi-account material-icons-name"></i></label><br>
                            <input name="FirstName" id="name" placeholder="Firstname" required><br>
                        </div>
                        <div class="form-group">

                            <label for=" Lastname"><i class="zmdi zmdi-account material-icons-name"></i></label><br>
                            <input name="LastName" placeholder="Lastname" required><br>

                        </div>
                        <div class="form-group">


                            <label for="Username"><i class="zmdi zmdi-account material-icons-name"></i></label><br>
                            <input name="Username" placeholder="Username" required><br>
                        </div>
                        <div class="form-group">

                            <label for=" Email"><i class="zmdi zmdi-email"></i></label><br>
                            <input name="Email" placeholder="Email" required><br>
                        </div>
                        <div class="form-group">

                            <label for="Password"><i class="zmdi zmdi-lock"></i></label><br>
                            <input name="Password" id="pass" placeholder="Password" required><br>
                        </div>
                        <div class="form-group">

                            <label for="RetypePassword"><i class="zmdi zmdi-lock-outline"></i></label><br>
                            <input name="RetypePassword" placeholder="Repeat your password" required><br>
                        </div>



                        <div class="form-group">



                            <label for="Address"><i class="zmdi zmdi-account material-icons-name"></i></label><br>
                            <input name="Address" placeholder=" Address" required><br>



                        </div>
                        <div class="form-group">
                            <label for=" Mobile"> <i class="zmdi zmdi-account material-icons-name"></i></label><br>
                            <input name="Mobile" placeholder="Mobile" required><br>
                        </div>
                        <div class="form-group">

                            <label for=" Gender"><i class="zmdi zmdi-account material-icons-name"></i></label><br>
                            <input name="Gender" placeholder="Male or Female" required><br>



                        </div>



                        <div class="form-group">
                            <label for="Age ">Age:</label><br>
                            <label>&nbsp&nbsp</label>
